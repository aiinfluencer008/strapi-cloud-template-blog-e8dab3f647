{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": "Organize your content into categories"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "description": {
      "type": "richtext"
    },
    "image": {
      "type": "string"
    },
    "metaTitle": {
      "type": "string"
    },
    "metaDescription": {
      "type": "text"
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog.blog",
      "mappedBy": "category"
    },
    "publishDate": {
      "type": "date"
    },
    "faqs": {
      "type": "component",
      "component": "faqs.faqs",
      "repeatable": true
    },
    "language": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::language.language"
    }
  }
}
